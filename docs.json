{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "Whispbot Documentation",
  "colors": {
    "primary": "#a33ce8",
    "light": "#a33ce8",
    "dark": "#a33ce8"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "pages": [
          "/welcome/landing",
          {
            "group": "Configuration",
            "pages": [
              "/config/home",
              "/config/modules",
              "/config/perms",
              "/config/moderation",
              "/config/shifts",
              "/config/roblox-moderation",
              "/config/erlc"
            ]
          },
          {
            "group": "Miscellaneous",
            "pages": ["/misc/faq", "/misc/troubleshooting"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "pages": ["api/landing"]
      },
      {
        "tab": "Changelog",
        "pages": ["/changes/updates"]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://whisp.bot/support"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://whisp.bot/dashboard"
    }
  },
  "footer": {
    "socials": {
      "discord": "https://whisp.bot/invite",
      "github": "https://github.com/Whispbot/Whispbot",
      "website": "https://whisp.bot"
    }
  },
  "background": {
    "decoration": "grid",
    "color": {
      "light": "#dccce4",
      "dark": "#0d021f"
    }
  },
  "styling": {
    "latex": true
  },
  "seo": {
    "indexing": "navigable"
  }
}
